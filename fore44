import pandas as pd
import matplotlib.pyplot as plt

# Load the data
df = pd.read_csv("your_data.csv", parse_dates=["Date"])  # Ensure 'Date' column is parsed correctly

# Define the dates to highlight
highlight_dates = pd.to_datetime(["2025-05-02", "2025-05-09", "2025-05-16", "2025-05-23"])

# Total count of entries per date
total_per_date = df["Date"].value_counts().sort_index()

# Highlighted entries
highlight_df = df[df["Date"].isin(highlight_dates)]
highlight_counts = highlight_df["Date"].value_counts().sort_index()

# Plot
plt.figure(figsize=(12, 6))
plt.bar(total_per_date.index, total_per_date.values, label="Total", color="lightgray")
plt.bar(highlight_counts.index, highlight_counts.values, label="Highlighted Dates", color="orange")
plt.xticks(rotation=45)
plt.xlabel("Date")
plt.ylabel("Count")
plt.title("Total vs Highlighted Dates")
plt.legend()
plt.tight_layout()
plt.show()
