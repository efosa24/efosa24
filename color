IF [seriousness] IN ('Adverse Event', 'AE Level 1', 'AE Level 2', 'AE Level 3', 'Serious AE') THEN 'Adverse Event'
ELSEIF [seriousness] IN ('Non-Serious', 'Serious', 'Lack of Effect', 'Priority', 'Non-Priority') THEN 'PQC'
ELSEIF [seriousness] = 'Preference' THEN 'Preference'
ELSE NULL
END


IF [Enterprise] = 'Adverse Event' AND NOT ([reg_class] IN ('MEDICAL DEVICE', 'MEDICAL DEVICE II')) THEN FALSE
ELSE TRUE
END
