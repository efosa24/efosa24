// Calculate the percentage of On-Time (Not Late)
LET on_time_percent = SUM(IF [Late?] = "Not Late" THEN 1 ELSE 0 END) * 100 / COUNT([Late?]);

// Assign colors
IF [Late?] = "Not Late" THEN 
    CASE 
        WHEN on_time_percent >= 95 THEN "Green"
        WHEN on_time_percent >= 90 THEN "Yellow"
        ELSE "Red"
    END
ELSE 
    "Blue"
END




